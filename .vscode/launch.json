{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit: App",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "${file}",
                "--server.port",
                "8501"
            ],
            "pyenv": "${command:python.interpreterPath}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}